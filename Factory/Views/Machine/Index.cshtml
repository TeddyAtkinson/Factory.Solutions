@{
  Layout = "_Layout";
}
<h1>Dr. Sillystringz's Factory</h1>
<h2>Machine Center</h2>
@using Factory.Models

@if (Model.Count == 0)
{
  <p>You have no Machines.</p>
}
else
{
  <ul>
    @foreach (Machine machine in Model)
    {
      <li>@Html.ActionLink($"{machine.Name}", "Details", new { id = machine.MachineId })</li>
    }
  </ul>
}

<p>@Html.ActionLink("Add machine", "Create")</p>